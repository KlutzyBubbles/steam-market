{
  "name": "steam-market",
  "version": "3.0.3",
  "type": "module",
  "description": "Steam market API client",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislav-puzyrev/steam-market.git"
  },
  "author": "Vladislav Puzyrev",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "steam",
    "market",
    "api",
    "client",
    "sdk"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check": "npm run typecheck && npm run lint && npm run format",
    "clean": "rimraf build",
    "format": "prettier --check .",
    "lint": "eslint .",
    "prepack": "npm run clean && npm run build && npm run check",
    "prepare": "husky",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "currency-codes": "^2.2.0",
    "iso-639-1": "^3.1.5"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.2",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "eslint-config-vladpuz": "^2.0.3",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-config-vladpuz": "^1.0.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
